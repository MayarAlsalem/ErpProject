<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ERP</title>
    <link rel="stylesheet" href="~/css/START.css">
    <link rel="stylesheet" href="~/css/PERSON.css">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/11.3.10/sweetalert2.min.css">
 
    @RenderSection("CssFiles", required: false)
</head>
<body>

    <div class="container">
        <!------Nav------->
        <div class="nav">
            <ul>
                <li>
                    <a href="#">
                        <span class="icon"><img src="~/img/Alyousser - BR - لوغو الشركة-19-11.png" width="100%" height="100%"></span>
                        <span class="title">اليسر للارتقاء الهندسي </span>
                    </a>
                </li>
                <li>
                    <a href="ERP.html">
                        <span class="icon"><ion-icon name="business-outline"></ion-icon></span>
                        <span class="title">المشاريع</span>
                    </a>
                </li>
                <li>
                    <a asp-action="Index" asp-controller="Pe">
                        <span class="icon"><ion-icon name="people-circle-outline"></ion-icon></span>
                        <span class="title">المراجع </span>
                    </a>
                </li>
                <li>
                    <a asp-action="Index" asp-controller="Ev">
                        <span class="icon"><ion-icon name="bookmarks"></ion-icon></span>
                        <span class="title">الأحداث</span>
                    </a>
                </li>
                <li>
                    <a href="DUTY.html">
                        <span class="icon"><ion-icon name="calendar"></ion-icon></span>
                        <span class="title">الواجبات</span>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="log-out-outline"></ion-icon></span>
                        <span class="title">تسجيل خروج </span>
                    </a>
                </li>
            </ul>
        </div>
        <!-----End Nav------->
        <!----main---->
        <div class="main">
            <div class="topbar">
                <!-------Menu--------->
                <div class="toggle">
                    <ion-icon name="menu-outline"></ion-icon>
                </div>
                <!-----End Menu------>
                <!---DarkMode-LightMode--->
                <div class="user">
                    <input type="checkbox" class="checkbox" id="checkbox" name="">
                    <label for="checkbox" class="label">
                        <ion-icon name="moon-outline"></ion-icon>
                        <ion-icon name="sunny-outline"></ion-icon>
                        <div class="ball"></div>
                    </label>
                </div>
                <!-----End DarkMode-LightMode------>
            </div>
            <div class="crud">
                @RenderBody()
            </div>
        </div>
        <!------End Main-------->
    </div>
    @*<div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>*@

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/gsap.min.js"></script>
    <script type="text/javascript" src="~/js/START.js"></script>
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    @RenderSection("Scripts", required: false)





    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/select2@4.0.13/dist/css/select2.min.css" />
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/select2@4.0.13/dist/js/select2.min.js"></script>
    <script type="text/javascript">
        $(function () {
            $("#ddlCustomers").select2();
        });
        $(function () {
            $("#ddlCustomers1").select2();
        });
        $(function () {
            $("#ddlCustomers2").select2();
        });
    </script>
    <script src="~/js/site.js"type="text/javascript"></script>
</body>
</html>
